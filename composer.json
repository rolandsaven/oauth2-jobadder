{
    "name": "rolandsaven/oauth2-jobadder",
    "description": "JobAdder OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "jobadder",
        "jobadder api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Kalocsaven",
            "email": "rolandka@live.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "RolandSaven\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RolandSaven\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
